<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Warm_Rating</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL( Rating , &quot;Warm&quot;),AnnualRevenue  &lt;= 50000   ||  AnnualRevenue   &gt;=  100000)</errorConditionFormula>
    <errorDisplayField>AnnualRevenue</errorDisplayField>
    <errorMessage>Warm_Rating should be between 50000 to 1 Lakh</errorMessage>
</ValidationRule>
