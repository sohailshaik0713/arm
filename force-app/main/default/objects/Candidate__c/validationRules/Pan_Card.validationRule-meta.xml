<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Pan_Card</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(Pan_Card__c, &quot;^[A-Z]{5}[0-9]{4}[A-Z]{1}$&quot;))</errorConditionFormula>
    <errorDisplayField>Pan_Card__c</errorDisplayField>
    <errorMessage>&quot;Please enter a valid PAN card number.&quot;</errorMessage>
</ValidationRule>
