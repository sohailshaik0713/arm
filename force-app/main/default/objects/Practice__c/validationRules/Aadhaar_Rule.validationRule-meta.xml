<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Aadhaar_Rule</fullName>
    <active>false</active>
    <errorConditionFormula>(LEN( Aadhaar__c ) != 12) &amp;&amp; NOT( REGEX(  Aadhaar__c , &quot;[0-9]{12}&quot;) )</errorConditionFormula>
    <errorDisplayField>Aadhaar__c</errorDisplayField>
    <errorMessage>Aadhaar Number</errorMessage>
</ValidationRule>
